{
  "$schema": "./node_modules/release-it/schema/release-it.json",
  "git": {
    "requireCleanWorkingDir": true,
    "commitMessage": "release: v${version}",
    "tagName": "v${version}"
  },
  "github": {
    "release": true,
    "body": ""
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "@release-it/bumper": {
      "out": {
        "file": "ngsw-config.json",
        "path": "appData.version"
      }
    }
  }
}
