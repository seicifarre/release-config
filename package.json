{
  "name": "release-config",
  "version": "1.0.1",
  "description": "Automatizaci√≥n de releases para entorno con ramas master y develop usando release-it",
  "type": "module",
  "scripts": {
    "release": "node scripts/release.js",
    "release:master": "dotenv -- release-it --config .release-it.master.ts --ci --no-increment",
    "release:dev": "dotenv -- release-it --config .release-it.dev.ts --ci --no-increment"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "10.0.1",
    "dotenv": "16.5.0",
    "dotenv-cli": "8.0.0",
    "release-it": "19.0.1",
    "semver": "7.7.1",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  }
}
