{
  "name": "release-config",
  "version": "1.0.3-dev",
  "description": "Automatizaci√≥n de releases para entorno con ramas master y develop usando release-it",
  "type": "module",
  "scripts": {
    "release": "node scripts/release.js",
    "release:master": "dotenv -- release-it --ci --no-increment",
    "release:dev": "dotenv -- release-it --ci --no-increment --preRelease"
  },
  "dependencies": {
    "dotenv": "16.5.0"
  },
  "devDependencies": {
    "@release-it/bumper": "7.0.5",
    "@release-it/conventional-changelog": "10.0.1",
    "dotenv-cli": "8.0.0",
    "release-it": "19.0.1",
    "semver": "7.7.1"
  }
}
